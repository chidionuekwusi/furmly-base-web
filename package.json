{
  "name": "furmly-base-web",
  "version": "1.0.6",
  "description": "Base web library for furmly web themes",
  "main": "dist/bundle.js",
  "homepage": "https://chidionuekwusi.github.io/furmly-base-web",
  "scripts": {
    "build-web": "build-storybook -c .storybook -o docs",
    "deploy": "npm run build-web && gh-pages -d docs",
    "build": "node ./scripts/build.js",
    "test": "node ./node_modules/jest/bin/jest",
    "storybook": "npm run fixtures && start-storybook -p 9001 -c .storybook",
    "fixtures": "./node_modules/.bin/babel-node  ./stories/utils/createFixtures.js --presets=babel-preset-react,babel-preset-env"
  },
  "keywords": [
    "furmly"
  ],
  "author": "Chidi Onuekwusi",
  "license": "ISC",
  "dependencies": {
    "furmly-client": "git+https://github.com/chidionuekwusi/furmly-client.git",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.4.2",
    "react-calendar": "^2.17.1",
    "react-dom": "^16.4.2",
    "react-router-redux": "^4.0.8",
    "styled-components": "^3.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "@storybook/react": "^3.4.10",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "copy-webpack-plugin": "^4.5.3",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "gh-pages": "^2.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ]
  }
}
